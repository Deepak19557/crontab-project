<br>
<table>
   <thead class="thead-dark">
     <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Age</th>
        <th>Phone</th>
     </tr>
   </thead>

  <tbody class="user_list">
    <% @users.each do |user| %>        
       <tr> 
          <td><%= user.name %></td>
          <td><%= user.email %></td>
          <td><%= user.age %></td>
          <td><%= user.phone %></td>
       </tr>
     <% end %>
  </tbody>
</table>
<br>
